{
    "key": "${addon.key}",
    "baseUrl": "${addon.base-url}",
    "name": "Scrum Poker for Jira",
    "description": "Scrum Poker for Jira enables agile teams to collaboratively estimate their work",
    "vendor": {
        "name": "codescape",
        "url": "https://www.codescape.de"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed",
        "uninstalled": "/uninstalled"
    },
    "scopes": [
        "READ",
        "WRITE"
    ],
    "apiMigrations": {
        "gdpr": true
    },
    "modules": {
        "generalPages": [
            {
                "url": "/index.html",
                "key": "home",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "Scrum Poker for Jira"
                }
            }
        ],
        "webItems": [
            {
                "key": "scrum-poker-issue-start",
                "location": "jira.issue.tools",
                "weight": 10,
                "name": {
                    "value": "Scrum Poker starten"
                },
                "url": "/scrum-poker-session/start"
            }
        ]
    }
}
